[...leftRange, DOTS, lastPageIndex];\n    }\n\n    if (shouldShowLeftDots && !shouldShowRightDots) {\n      \n      let rightItemCount = 3 + 2 * siblingCount;\n      let rightRange = range(\n        totalCount - rightItemCount + 1,\n        totalCount\n      );\n      return [firstPageIndex, DOTS, ...rightRange];\n    }\n\n    if (shouldShowLeftDots && shouldShowRightDots) {\n      let middleRange = range(leftSiblingIndex, rightSiblingIndex);\n      return [firstPageIndex, DOTS, ...middleRange, DOTS, lastPageIndex];\n    }\n\n  }, [totalCount, pageSize, siblingCount, currentPage])\n\n  return paginationRange\n}","/home/nicoa/Documentos/projects/scapa-web/src/themes/theme.config.js",[],"/home/nicoa/Documentos/projects/scapa-web/src/themes/colors.js",[],"/home/nicoa/Documentos/projects/scapa-web/src/themes/breakpoints.js",[],"/home/nicoa/Documentos/projects/scapa-web/src/themes/typografy.js",[],"/home/nicoa/Documentos/projects/scapa-web/src/router/index.js",[],"/home/nicoa/Documentos/projects/scapa-web/src/layouts/workspace.js",[],{"ruleId":"243","replacedBy":"244"},{"ruleId":"245","replacedBy":"246"},{"ruleId":"247","severity":1,"message":"248","line":14,"column":5,"nodeType":"249","endLine":14,"endColumn":7,"suggestions":"250"},{"ruleId":"251","severity":1,"message":"252","line":11,"column":11,"nodeType":"253","messageId":"254","endLine":11,"endColumn":18},{"ruleId":"251","severity":1,"message":"255","line":15,"column":10,"nodeType":"253","messageId":"254","endLine":15,"endColumn":19},{"ruleId":"247","severity":1,"message":"256","line":19,"column":6,"nodeType":"249","endLine":19,"endColumn":13,"suggestions":"257"},{"ruleId":"247","severity":1,"message":"258","line":58,"column":6,"nodeType":"249","endLine":58,"endColumn":31,"suggestions":"259"},{"ruleId":"247","severity":1,"message":"258","line":47,"column":6,"nodeType":"249","endLine":47,"endColumn":31,"suggestions":"260"},{"ruleId":"251","severity":1,"message":"261","line":13,"column":10,"nodeType":"253","messageId