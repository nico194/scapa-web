ch({\n\t\t\t\t\ttype: FETCH_ALL_CATEGORIES_SUCCESS,\n\t\t\t\t\tpayload: {\n\t\t\t\t\t\tallCategories: response.data.data\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t)\n\t\t\t.catch(err => {\n\t\t\t\tdispatch({ type: FETCH_CATEGORIES_ERROR, payload: { err: err.response.data.error } })\n\t\t\t});\n\t}\n}\n\nexport const addCategory = (categoryDescription, { accessToken, client, uid }) => {\n\treturn dispatch => {\n\t\tdispatch({ type: FETCH_CATEGORIES_PENDING });\n\t\tconst headers = {\n\t\t\theaders: {\n\t\t\t\t'access-token': accessToken,\n\t\t\t\tclient,\n\t\t\t\tuid\n\t\t\t}\n\t\t}\n\t\tconst category = {\n\t\t\tcategory: {\n\t\t\t\tdescription: categoryDescription.attributes.description\n\t\t\t}\n\t\t}\n\t\taxiosConfig.post('/admin/categories', JSON.stringify(category), headers)\n\t\t\t.then(response => dispatch({ type: ADD_CATEGORY_SUCCESS, payload: { category: response.data.data } }))\n\t\t\t.catch(err => {\n\t\t\t\tdispatch({ type: FETCH_CATEGORIES_ERROR, payload: { err: err.response.data.error }})\n\t\t\t});\n\t}\n}\n\nexport const deleteCategory = (id, { accessToken, client, uid }) => {\n\treturn dispatch => {\n\t\tdispatch({ type: FETCH_CATEGORIES_PENDING });\n\t\tconst headers = {\n\t\t\theaders: {\n\t\t\t\t'access-token': accessToken,\n\t\t\t\tclient,\n\t\t\t\tuid\n\t\t\t}\n\t\t}\n\t\taxiosConfig.delete(`/admin/categories/${id}`, headers)\n\t\t\t.then(response => dispatch({ type: DELETE_CATEGORY_SUCCESS, payload: { category: response.data.data } }))\n\t\t\t.catch(err => dispatch({ type: DELETE_CATEGORY_ERROR, payload: { err: err.response.data.error } }));\n\t}\n}\n\nexport const updateCategory = (categoryToUpdate, { accessToken, client, uid }) => {\n\treturn dispatch => {\n\t\tdispatch({ type: FETCH_CATEGORIES_PENDING });\n\t\tconst headers = {\n\t\t\theaders: {\n\t\t\t\t'access-token': accessToken,\n\t\t\t\tclient,\n\t\t\t\tuid\n\t\t\t}\n\t\t}\n\t\tconst category = {\n\t\t\tcategory: {\n\t\t\t\tdescription: categoryToUpdate.attributes.description\n\t\t\t}\n\t\t}\n\t\taxiosConfig.put(`/admin/categories/${categoryToUpdate.id}`, JSON.stringify(category), headers)\n\t\t\t.then(response => dispatch({ type: UPDATE_CATEGORY_SUCCESS, payload: { category: response.data.data } }))\n\t\t\t.catch(err => dispatch({ type: UPDATE_CATEGORY_ERROR, payload: { err: err.response.data.error } }));\n\t}\n}\n\nexport const clearError = () => dispatch => dispatch({ type: CLEAR_ERROR })\n","/Users/nalarcon/Documents/projects/scapa-web/src/redux/actions/users.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/redux/actions/routines.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/atoms/input/Input.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/atoms/button/Button.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/atoms/upload-image/UploadImage.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/atoms/spinner/Spinner.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/molecules/modal/Modal.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/atoms/select/Select.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/organisms/header/Header.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/molecules/card/Card.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/organisms/pictogram/Pictogram.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/redux/constants/users.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/redux/constants/pictograms.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/redux/constants/categories.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/redux/constants/routines.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/configs/axios.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/configs/navigation.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/components/organisms/table/Table.js",[],"/Users/nalarcon/Documents/projects/scapa-web/src/